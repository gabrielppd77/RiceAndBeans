{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Secret": "SECRET-JWT",
    "ExpiryMinutes": 120,
    "Issuer": "https://app-generate-token.com",
    "Audience": "https://who-use-token.com"
  },
  "UploadFileSettings": {
    "BaseUrl": "local-image-service.com",
    "AccessKey": "ACCESS_KEY",
    "SecretKey": "SECRET_KEY"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost:5432;Database=rice-and-beans;Username=postgres;Password=123456;Include Error Detail=true"
  },
  "EmailSettings": {
    "From": "app-email@gmail.com",
    "DisplayName": "app-name",
    "Host": "smtp.gmail.com",
    "Port": 587,
    "UserName": "smtp-email@gmail.com",
    "Password": "smtp-email-password",
    "EnableSsl": true
  },
  "FrontendSettings": {
    "ConfirmEmailUrl": "https://domain.com/confirm-email",
    "PasswordRecoveryUrl": "https://domain.com/recover-password"
  }
}
